<?xml version="1.0" encoding="ISO-8859-1"?>

<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>

  <display-name>TagUnit blank</display-name>
  <description>This webapp is a starting point for testing your own taglibs</description>

  <servlet>
    <servlet-name>TagUnitTestController</servlet-name>
    <servlet-class>org.tagunit.controller.FrontController</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>TagUnitTestController</servlet-name>
    <url-pattern>/test/servlet/*</url-pattern>
  </servlet-mapping>

  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>

</web-app>
